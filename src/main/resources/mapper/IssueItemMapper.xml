<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.master.track.mapper.IssueItemMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.master.track.entity.IssueItem">
        <id column="id" property="id" />
        <result column="project_name" property="projectName" />
        <result column="module" property="module" />
        <result column="function_desc" property="functionDesc" />
        <result column="title_desc" property="titleDesc" />
        <result column="severity" property="severity" />
        <result column="owner" property="owner" />
        <result column="status" property="status" />
        <result column="data" property="data" />
        <result column="remark" property="remark" />
        <result column="create_date" property="createDate" />
        <result column="update_date" property="updateDate" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, project_name, module, function_desc, title_desc, severity, owner, status, data, remark, create_date, update_date
    </sql>

</mapper>
